
bindir          = @bindir@
datadir		= @datadir@
datarootdir	= @datarootdir@
exec_prefix     = @exec_prefix@
includedir      = @includedir@
libdir          = @libdir@
mandir          = @mandir@
srcdir		= @srcdir@
prefix          = @prefix@

INSTALL		= @INSTALL@
MKDIR_P		= @MKDIR_P@
VPATH		= $(srcdir)
RM		= rm -f

version		= @PACKAGE_VERSION@

FILES		= \
		$(srcdir)/library.xml

destdir		= $(libdir)/Rappture$(version)/lib

all: 

install: 
	$(MKDIR_P) $(destdir)
	for i in $(FILES) ; do \
	  $(INSTALL) -m 444  $$i $(destdir) ; \
	done

clean:

distclean: 
	$(RM) Makefile *~

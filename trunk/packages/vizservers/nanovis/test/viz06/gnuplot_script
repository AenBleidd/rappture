set out "graph.ps"
set size 1.0, 0.4
set terminal postscript portrait enhanced mono lw 2 "Helvetica" 14

set key 80,0.12
set title "Performance of rendering servers"
set xlabel "Number of simultaneous clients"
set ylabel "Average time per frame (seconds)"
set label "Hit physical memory limit" at 10,0.3
set arrow from 16.5,0.29 to 16.8,0.27
plot "results1" using 1:2 title '1 rendering machine' w linespoints, \
     "results3" using 1:2 title '3 rendering machines' w linespoints, \
     "results5" using 1:2 title '5 rendering machines' w linespoints



bindir          = @bindir@
datadir         = @datadir@
datarootdir     = @datarootdir@
exec_prefix     = @exec_prefix@
includedir      = @includedir@
libdir          = @libdir@
mandir          = @mandir@
srcdir          = @srcdir@
prefix          = @prefix@

INSTALL     = @INSTALL@
MKDIR_P     = @MKDIR_P@
VPATH       = $(srcdir)
RM          = rm -f

FILES       = \
		$(srcdir)/loadrun.tcl \
		$(srcdir)/run5.xml \
		$(srcdir)/run10.xml \
		$(srcdir)/run20.xml \
		$(srcdir)/tool.xml \

destdir     = $(prefix)/examples/zoo/loadrun

all:

install:
	$(MKDIR_P) $(destdir)
	for i in $(FILES) ; do \
	  $(INSTALL) -m 444  $$i $(destdir) ; \
	done

clean:

distclean:
	$(RM) Makefile *~

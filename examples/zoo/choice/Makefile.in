
bindir          = @bindir@
datadir		= @datadir@
datarootdir	= @datarootdir@
exec_prefix     = @exec_prefix@
includedir      = @includedir@
libdir          = @libdir@
mandir          = @mandir@
srcdir		= @srcdir@
prefix          = @prefix@

INSTALL		= @INSTALL@
INSTALL_DATA	= @INSTALL_DATA@
VPATH		= $(srcdir)
RM		= rm -f

FILES		= \
		$(srcdir)/choice.gif \
		$(srcdir)/choice.tcl \
		$(srcdir)/tool.xml

destdir		= $(prefix)/examples/zoo/choice

all: 

install: 
	$(INSTALL) -d $(destdir)
	for i in $(FILES) ; do \
	  $(INSTALL) -m 444  $$i $(destdir) ; \
	done

clean:

distclean: 
	$(RM) Makefile *~


bindir          = @bindir@
datadir         = @datadir@
datarootdir     = @datarootdir@
exec_prefix     = @exec_prefix@
includedir      = @includedir@
libdir          = @libdir@
mandir          = @mandir@
prefix          = @prefix@
srcdir          = @srcdir@

destdir         = $(prefix)/examples/app-fermi/octave

INSTALL         = @INSTALL@
MKDIR_P         = @MKDIR_P@

SUBDIRS		= 2 3

all: 
	for i in $(SUBDIRS) ; do \
	  $(MAKE) -C $$i all || exit 1 ;\
	done

install: 
	for i in $(SUBDIRS) ; do \
	  $(MAKE) -C $$i install || exit 1 ;\
	done

clean:
	for i in $(SUBDIRS) ; do \
	  $(MAKE) -C $$i clean ;\
	done

distclean: clean
	$(RM) Makefile


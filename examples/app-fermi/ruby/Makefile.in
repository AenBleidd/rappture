
bindir          = @bindir@
datadir		= @datadir@
datarootdir	= @datarootdir@
exec_prefix     = @exec_prefix@
includedir      = @includedir@
libdir          = @libdir@
mandir          = @mandir@
prefix          = @prefix@
srcdir		= @srcdir@

destdir		= $(prefix)/examples/app-fermi/ruby

INSTALL		= @INSTALL@

FILES 		= \
		$(srcdir)/tool.xml \
		$(srcdir)/fermi.rb

all:	

install: 
	$(INSTALL) -d $(destdir)
	for i in $(FILES) ; do \
	  $(INSTALL) -m 444 $$i $(destdir) ; \
	done

clean:

distclean: clean
	$(RM) Makefile


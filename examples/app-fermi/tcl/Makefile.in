
bindir          = @bindir@
datadir		= @datadir@
datarootdir	= @datarootdir@
exec_prefix     = @exec_prefix@
includedir      = @includedir@
libdir          = @libdir@
mandir          = @mandir@
prefix          = @prefix@
srcdir		= @srcdir@

INSTALL		= @INSTALL@
MKDIR_P		= @MKDIR_P@
VPATH		= @srcdir@

destdir		= $(prefix)/examples/app-fermi/tcl
FILES 		= \
		$(srcdir)/tool.xml \
		$(srcdir)/fermi.tcl

all:	
install: 
	$(MKDIR_P) $(destdir)
	for i in $(FILES) ; do \
	  $(INSTALL) -m 444  $$i $(destdir) ; \
	done

clean:

distclean: clean
	$(RM) Makefile



bindir          = @bindir@
datadir		= @datadir@
datarootdir	= @datarootdir@
exec_prefix     = @exec_prefix@
includedir      = @includedir@
libdir          = @libdir@
mandir          = @mandir@
prefix          = @prefix@
srcdir		= @srcdir@

destdir		= $(prefix)/examples/app-fermi/2.0

INSTALL		= @INSTALL@
MKDIR_P		= @MKDIR_P@

FILES 		= \
		$(srcdir)/README \
		$(srcdir)/fermi.py \
		$(srcdir)/fermi_io.py

all:	

install: 
	$(MKDIR_P) $(destdir)
	for i in $(FILES) ; do \
	  $(INSTALL) -m 444 $$i $(destdir) ; \
	done

clean:

distclean: clean
	$(RM) Makefile



bindir          = @bindir@
datadir		= @datadir@
datarootdir	= @datarootdir@
exec_prefix     = @exec_prefix@
includedir      = @includedir@
libdir          = @libdir@
mandir          = @mandir@
prefix          = @prefix@
srcdir		= @srcdir@

INSTALL		= @INSTALL@
VPATH		= $(srcdir)
SHELL		= @SHELL@
RM		= rm -f

.PHONY:	cee python tcl

all:	python tcl cee

cee:
	$(MAKE) -C cee all
python: 
	$(MAKE) -C python all
tcl: 
	$(MAKE) -C tcl all

install:
	$(MAKE) -C cee install
	$(MAKE) -C python install
	$(MAKE) -C tcl install

clean:
	$(MAKE) -C cee clean
	$(MAKE) -C python clean
	$(MAKE) -C tcl clean

distclean: 
	$(MAKE) -C cee distclean
	$(MAKE) -C python distclean
	$(MAKE) -C tcl distclean
	$(RM) Makefile

            <preset>
                <label>300K (room temperature)</label>
                <value>300K</value>
            </preset>
            <preset>
                <label>77K (liquid nitrogen)</label>
                <value>77K</value>
            </preset>
            <preset>
                <label>4.2K (liquid helium)</label>
                <value>4.2K</value>
            </preset>
        </number>
    </input>
</run>

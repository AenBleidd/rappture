<?xml version="1.0"?>
<run>
    <input>
        <number id="Ef">
            <about>
                <label>Fermi Level</label>
                <description>Energy at center of distribution.</description>
            </about>
            <units>eV</units>
            <min>-10eV</min>
            <max>10eV</max>
